<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.exploresg">
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ANDE"
        android:windowBackground="#FFFFFF">
        <activity android:name="com.example.exploresg.activities.SplashScreen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.exploresg.activities.MainActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.example.exploresg.activities.SavedLocationsActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.example.exploresg.activities.SettingActivity">

        </activity>
        <activity android:name="com.example.exploresg.activities.RecoPageActivity">

        </activity>
        <activity android:name="com.example.exploresg.activities.AboutActivity">

        </activity>
        <activity android:name="com.example.exploresg.activities.databaseTestActivity">

        </activity>
        <activity android:name="com.example.exploresg.activities.RecoDetailActivity">

        </activity>
        <activity android:name="com.example.exploresg.activities.NotificationActivity">

        </activity>
        <activity android:name="com.example.exploresg.activities.HistoryActivity">

        </activity>
        <service
            android:name="com.example.exploresg.notification.NotificationService"
            android:permission="android.permission.BIND_JOB_SERVICE"
            >
        </service>
        <receiver
            android:name="com.example.exploresg.notification.NotificationReceiver"
            android:enabled="true"
            android:exported="true" >
        </receiver>
    </application>

</manifest>